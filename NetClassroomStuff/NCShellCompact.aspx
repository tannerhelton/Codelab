fld = document.getElementById(container + '_state');
orient = document.getElementById(container + '_orient');
anim = document.getElementById(container + '_anim');
msec = document.getElementById(container + '_animtime').value;
step = document.getElementById(container + '_animstep').value;
img = document.getElementById(container + '_button');
panelObject=document.getElementById(container + '_collapsible');
panelObject2=document.getElementById(container + '_collapsible2');
vContainer=document.getElementById('');
