// -->

function getHeight(object) {
return parseInt(object.style.height);
}

function getWidth(object) {
return parseInt(object.style.width);
}

function WipeIt(container, type) {
h=getHeight(panelObject2);
w=getWidth(panelObject2);
type=type.toLowerCase();
if (orient.value=="vert")
{
if (isNaN(h)==true)
{
alert('You must specify a height for animated vertically collapsing panels');
}
else
{
switch (type) {
case "block": 
CollapseVertPanel();
break;
case "none": 
panelObject.style.clip="rect(auto,auto,0,auto)"; 
panelObject.style.display = "block";
panelObject2.style.display = "block";
ExpandVertPanel();
break;
}
}
}
else 
{
if (isNaN(w)==true)
{
alert('You must specify a width for animated horizontally collapsing panels');
}
else
{
switch (type) {
case "block": 
CollapseHorzPanel();
break;
case "none": 
if (vContainer){
vContainer.width=w;
}
panelObject.style.clip="rect(auto,0,auto,auto)"; 
panelObject.style.display = "block";
panelObject2.style.display = "block";
ExpandHorzPanel();
break;
}
}
}
}

function ExpandVertPanel() {
if (i<step) {
i++;
b=h/step*i;
panelObject.style.clip="rect(auto auto " + b + " auto)"; 
setTimeout("ExpandVertPanel()", msec/step);
} else {
i=0;
FinishCollapseExpand();
}
}

function CollapseVertPanel() {
if (i<step) {
i++;
b=h-h/step*i;
panelObject.style.clip="rect(auto auto " + b + " auto)"; 
setTimeout("CollapseVertPanel()", msec/step);
} else {
i=0;
FinishCollapseExpand();
}
}

function ExpandHorzPanel() {
if (i<step) {
i++;
b=w/step*i;
panelObject.style.clip="rect(auto " + b + " auto auto)"; 
setTimeout("ExpandHorzPanel()", msec/step);
} else {
i=0;
FinishCollapseExpand();
}
}

function CollapseHorzPanel() {
if (i<step) {
i++;
b=w-w/step*i;
panelObject.style.clip="rect(auto " + b + " auto auto)"; 
setTimeout("CollapseHorzPanel()", msec/step);
} else {
i=0;
FinishCollapseExpand();
}
}

function FinishCollapseExpand() {
fld.value = (fld.value=="block") ? "none" : "block";
panelObject.style.display = fld.value;
panelObject2.style.display = fld.value;
img.src = (fld.value=="block") ? colimg : expimg;
if (orient.value=="horz"){
if (vContainer){
if (fld.value=="none"){
vContainer.width='';
}
}
}
}
</script>

